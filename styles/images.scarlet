.lazy
	visibility hidden
	opacity 0

.element-found
	visibility visible
	opacity 1 !important

.element-not-found
	visibility hidden
	opacity 0 !important

.image-loading
	object-position: center;
	object-fit contain !important
	width 100%
	height 100%
	border-radius 50%
	transform rotateY(0deg)
	animation im-rotate 500ms infinite alternate-reverse linear

animation im-rotate
	0%
	transform rotateY(0deg)
	100%
	transform rotateY(90deg)